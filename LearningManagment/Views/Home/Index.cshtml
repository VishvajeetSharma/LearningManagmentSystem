    <style>
        *{
            padding: 0;
            margin: 0;
            box-sizing: border-box;
            font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
        }
        body{
            overflow-x: hidden;
        }
        :root {
            --color1: rgb(231, 134, 80);
            --color2: rgb(85, 125, 203);
            --color3: blue;
            --color4: rgb(85, 125, 203, .45);
        }

        .bg-mycolor1 {
            background-color: var(--color1);
        }

        .bg-mycolor2 {
            background-color: var(--color2);
        }

        .bg-mycolor3 {
            background-color: var(--color3);
        }

        .bg-mycolor4 {
            background-color: var(--color4);
        }

        .txt-mycolor1 {
            color: var(--color1);
        }

        .txt-mycolor2 {
            color: var(--color2);
        }
        .ic {
            font-size: 18px;
            margin-right: 20px;
            width: 20px;
            line-height: 20px;
            transition: transform .5s ease-in-out;
        }
            .ic:hover {
                transform: scale(1.5);
            }
        .t1{
            font-size: 14px;
        }
        .t1 a{
            text-decoration: none;
            color: white;
        }
        #menu {
            background-color: transparent !important;
        }
        .nav-item{
            padding: 2px 10px;
            transition: transform .5s ease-in-out;
            border-radius: 20px;
        }
        .nav-item:hover{
            transform: scale(1.1);
            background-color: var(--color2);
            border-radius: 20px;
            transition: 1s;
        }
        .slider {
            min-height: 600px;
            background-image: url('../../content/images/image1.jpg');
            background-size: cover;
            background-attachment: fixed;
            margin-bottom: 5px;
        }
        .login{
            backdrop-filter: blur(5px);
            background-color: rgba(255, 255, 255, .2);
            margin-top: 10%;
            padding: 4%;
            color: white;
            border-radius: 20px;
        }
        .form-control{
            border-radius: 10px !important;
            height: 50px;
            margin-bottom: 10px;
        }
        .btn1{
            height: 50px;
            border: none;
            color: white;
            font-weight: 700;
            border-radius: 10px;
            font-family:'Times New Roman', Times, serif;
        }
        .sicon{
            height: 90px;
            width: 90px;
            background-color: var(--color2);
            text-align: center !important;
            line-height: 90px;
            border-radius: 50%;
            font-size: 40px;
            color: white;
        }
        .aicon {
            height: 70px !important;
            width: 70px !important;
            line-height: 70px !important;
            font-size: 35px !important;
        }
        .sec_heading{
            font-size: 18px;
            text-decoration: 2px dashed underline;
        }
        .about3 {
            background-color: var(--color4);
            text-align: center;
            border-radius: 15px;
            transition: all .8s;
        }
        .about3:hover{
            color: white;
            background-color: var(--color2);
            transition: all 1s;
            transform: translateY(-6px);
        }
        .bfoottxt{
            font-size: 14px;
            color: white;
            text-decoration: none;
        }
            .bfoottxt:hover {
                color: var(--color1);
                font-size: 16px;
                transition: all .6s;
            }
        .logo {
            height: 40px;
        }

            .logo:hover {
                transition: all 1.2s;
                transform: translateY(3px);
            }
        .btn:hover{
            background-color: var(--color2);
        }
        .overlay-top::before {
            position: absolute;
            content: "";
            width: 100%;
            height: 85px;
            left: 0;
            z-index: 1;
        }
        .overlay-top::before {
            top: 0;
            background: url(/Content/images/overlay-top.png) top center no-repeat;
            background-size: contain;
        }
        .bgform {
            background-image: linear-gradient(to bottom left, rgba(85, 125, 203, .4), rgba(280, 143 , 80, .2));
            border-radius: 10px;
        }
        .faqs{
            margin-top: 10px;
            padding: 30px 0px;
        }
        .bgcontact{
            background-image: linear-gradient(to bottom left, rgba(245, 240, 235, .3), rgba(85, 125, 200, .1));
            border-radius: 10px;
        }
        .fic{
            font-size: 25px;
            color: white;
            background-color: var(--color2);
            border-radius: 50%;
            padding: 23%;
        }
        .card:hover {
            color: white;
            background-color: var(--color2);
            transition: all 1s;
            transform: translateY(-10px);
        }
    </style>
